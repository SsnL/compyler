#!/bin/bash

DIR="$( cd "$( dirname "${BASH_SOURCE[0]}" )" && pwd )"

# Add local bin
export PATH=$DIR/root/bin/:$PATH

# Path to servers
SERVER=cs164-$1@torus.cs.berkeley.edu
PROJ_PATH="~/work/proj/proj2"
SRC_PATH="src"

function send {
  scp -r $1 "$SERVER":"$PROJ_PATH"/"$2"
}

function connect {
  ssh $SERVER
}
